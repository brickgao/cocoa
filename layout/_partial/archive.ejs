<div class="posts-list">
  <% page.posts.each(function(post, i) { %>
    <div class="post-summary">
      <div class="post-title">
        <a href="<%- url_for(post.path) %>"><%= post.title %></a>
      </div>
      <div class="post-date">
        <%= date(post.date, "MMM DD, YYYY") %>
      </div>
    </div>
  <% }) %>
</div>
